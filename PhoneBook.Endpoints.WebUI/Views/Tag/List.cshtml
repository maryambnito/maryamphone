@model List<PhoneBook.Domain.Core.Tags.Tag>
@{ }


@if (Model != null && Model.Count > 0)
{
    <table class="table table-bordered table-striped">
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Action</th>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>
                    <a asp-controller="Tag" asp-action="Delete" asp-route-id="@item.Id">Detele</a>
                </td>
            </tr>
          }

    </table>
}