@model List<PhoneBook.Domain.Core.Peoples.Person>
@{ 

}
@if (Model != null && Model.Count > 0)
{
<table class="table table-bordered table-striped">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <td>Actions</td>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.FristName</td>
    <td>@item.LastName</td>
    <td>@item.Email</td>
    <td>
        <a asp-controller="People" asp-action="Details" asp-route-id="@item.Id">Detail</a>
        |
        <a asp-controller="People" asp-action="Delete" asp-route-id="@item.Id">Detele</a>
        |
        <a asp-controller="People" asp-action="Update" asp-route-id="@item.Id">Edit</a>

    </td>
</tr>
}
</table>}
